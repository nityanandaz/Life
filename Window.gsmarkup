<gsmarkup>
  <objects>
    <window title="Game of Life" closable="no" resizable="no" width="400" height="400">
      <view width="400" height="400" instanceOf="GridView" id="gridView" lifeController="#lifeController"/>
    </window>
    <instance instanceOf="LifeController" id="lifeController" gridView="#gridView"/>
  </objects>
  <connectors>
    <outlet source="#NSOwner" target="#lifeController" key="lifeController" />
  </connectors>
</gsmarkup>
